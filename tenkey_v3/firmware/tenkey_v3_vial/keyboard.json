{
  "manufacturer": "wappon",
  "keyboard_name": "tenkey_v3",
  "maintainer": "wappon",

  "development_board": "promicro_rp2040",
  "processor": "RP2040",
  "bootloader": "rp2040",

  "diode_direction": "COL2ROW",

  "features": {
    "bootmagic": true,
    "extrakey": true,
    "mousekey": true,
    "nkro": true
  },

  "matrix_pins": {
    "cols": ["GP6", "GP7", "GP8", "GP9", "GP10"],   // col0..col4
    "rows": ["GP4", "GP3", "GP2", "GP1", "GP0"]     // row0..row4
  },

  "url": "",
  "usb": {
    "vid": "0xFEED",
    "pid": "0x0000",
    "device_version": "1.0.0"
  },

  "layouts": {
    "LAYOUT": {
      "layout": [
        { "matrix": [0,0], "x": 0, "y": 0 },
        { "matrix": [0,1], "x": 1, "y": 0 },
        { "matrix": [0,2], "x": 2, "y": 0 },
        { "matrix": [0,3], "x": 3, "y": 0 },
        { "matrix": [0,4], "x": 4, "y": 0 },   // ← col4 を使うキー（右上）

        { "matrix": [1,0], "x": 0, "y": 1 },
        { "matrix": [1,1], "x": 1, "y": 1 },
        { "matrix": [1,2], "x": 2, "y": 1 },
        { "matrix": [1,3], "x": 3, "y": 1 },

        { "matrix": [2,0], "x": 0, "y": 2 },
        { "matrix": [2,1], "x": 1, "y": 2 },
        { "matrix": [2,2], "x": 2, "y": 2 },
        { "matrix": [2,3], "x": 3, "y": 2 },

        { "matrix": [3,0], "x": 0, "y": 3 },
        { "matrix": [3,1], "x": 1, "y": 3 },
        { "matrix": [3,2], "x": 2, "y": 3 },
        { "matrix": [3,3], "x": 3, "y": 3 },

        { "matrix": [4,0], "x": 0, "y": 4 },
        { "matrix": [4,1], "x": 1, "y": 4 },
        { "matrix": [4,2], "x": 2, "y": 4 },
        { "matrix": [4,3], "x": 3, "y": 4 }

        // 行4×列5=25 のうち実際にハンダ付けした21キーだけレイアウトに載せる
        // [1,4], [2,4], [3,4], [4,4] は空きセルなので記載しない
      ]
    }
  }
}
